{
  "type": "module",
  "name": "medications-delivery",
  "version": "1.0.0",
  "description": "This project is a prototype API for a health logistics startup aiming to revolutionize medical supply chains using drones. The system enables the management of drones and the tracking of medications they transport, targeting efficient and reliable delivery to patients in remote or hard-to-reach areas.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "npx drizzle-kit migrate",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "medication-delivery"
  },
  "keywords": [
    "medication",
    "api",
    "delivery"
  ],
  "author": "Jojo Saka",
  "license": "UNLICENSED",
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "@types/express": "^5.0.3",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.6",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.20.0",
    "mongoose": "^8.19.0",
    "morgan": "^1.10.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "zod": "^4.1.11"
  },
